{
  "rewrites": [
    {
      "source": "/pricing/",
      "destination": "/pricing/index.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing/index.html"
    },
    {
      "source": "/review",
      "destination": "/api/review-redirect"
    },
    {
      "source": "/fb",
      "destination": "/api/fb-redirect"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.handyandfriend.com"
        }
      ],
      "destination": "https://handyandfriend.com/$1",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/pipeline-cron?action=sla",
      "schedule": "*/15 15-23,0-4 * * *"
    },
    {
      "path": "/api/pipeline-cron?action=report",
      "schedule": "0 17 * * 0"
    }
  ]
}
